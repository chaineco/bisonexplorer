{
    "extends": "standard",
    "parser": "@babel/eslint-parser",
    "globals": {
        "$": true
    },
    "rules": {
        "no-trailing-spaces": "error",
        "no-console": ["off"],
        "no-alert": "error",
        "no-eval": "error",
        "no-implied-eval": "error",
        "no-var": "error",
        "array-callback-return": "off",
        "object-shorthand": ["error", "consistent"]
    }
}