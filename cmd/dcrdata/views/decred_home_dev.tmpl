<div class="main-container new-homepage homepage-class homepage-content" id="content" data-controller="charts time"
   data-charts-tps="{{.TargetPoolSize}}" data-charts-svh="{{.ChainParams.StakeValidationHeight}}"
   data-charts-pos="{{.ChainParams.StakeRewardProportion}}" data-charts-premine="{{.Premine}}"
   data-charts-bs="{{.ChainParams.BaseSubsidy}}" data-charts-sri="{{.ChainParams.SubsidyReductionInterval}}"
   data-charts-mul-subsidy="{{.ChainParams.MulSubsidy}}" data-charts-div-subsidy="{{.ChainParams.DivSubsidy}}"
   data-charts-window-size="{{.ChainParams.StakeDiffWindowSize}}"
   data-charts-block-time="{{.ChainParams.TargetTimePerBlock.Seconds}}">
   <div class="hp-explore-wrap homepage-page pb-0">
      {{- if $botState -}}
      <!-- market chart start -->
      <div class="row w-100 align-items-top" data-controller="market"
         data-market-dcrbtcprice="{{$botState.DCRBTCPrice}}" data-market-dcrbtcvolume="{{$botState.DCRBTCVolume}}"
         data-market-price="{{$botState.Price}}" data-market-volume="{{$botState.Volume}}">
         {{- /* LEFT COLUMN */ -}}
         <div class="col-24 col-lg-auto p-0">
            {{- /* PRICE */ -}}
            <div class="ms-4 me-2 my-4 p-2 p-lg-4 bg-white frame-shadow">
               <div class="fs22 text-center pb-2"><img src="/images/dcr-icon.png" width="25" height="25" class="me-2">Market Information</div>
               <table class="w-100 mx-0">
                  <tbody>
                     <tr>
                        <td class="py-2 fs-16 fw-bold">Price</td>
                        <td class="ps-2 py-2 text-end">
                           <p class="fs-16">
                              ${{printf "%.2f" $botState.Price}} USD</span>
                              {{if eq $botState.DCRUSD24hChange 0.0}}
                              <span></span>
                              {{else if gt $botState.DCRUSD24hChange 0.0}}
                              <span class="dcricon-arrow-up text-green">{{printf "%.2f" (x100 (divideFloat $botState.DCRUSD24hChange $botState.Price))}}%</span>
                              {{else}}
                              <span class="dcricon-arrow-down text-danger">{{printf "%.2f" (x100 (divideFloat (toAbsValue $botState.DCRUSD24hChange) $botState.Price))}}%</span>
                              {{end}}
                           </p>
                           <p class="fs-14">{{printf "%.6f" $botState.DCRBTCPrice}} BTC</span>
                              {{if eq $botState.DCRBTC24hChange 0.0}}
                              <span></span>
                              {{else if gt $botState.DCRBTC24hChange 0.0}}
                              <span class="dcricon-arrow-up text-green">{{printf "%.2f" (x100 (divideFloat $botState.DCRBTC24hChange $botState.DCRBTCPrice))}}%</span>
                              {{else}}
                              <span class="dcricon-arrow-down text-danger">{{printf "%.2f" (x100 (divideFloat (toAbsValue $botState.DCRBTC24hChange) $botState.DCRBTCPrice))}}%</span>
                              {{end}}
                           </p>
                        </td>
                     </tr>
                     <tr>
                        <td class="py-2 fs-16 fw-bold">Market Cap</td>
                        <td class="ps-2 py-2 text-end">
                           <p class="fs-16">
                              {{if $conv -}}
                                 $<span>{{threeSigFigs $conv.CoinSupply.Value}} {{$conv.CoinSupply.Index}}</span>
                              {{- end}}</span>
                           </p>
                        </td>
                     </tr>
                     <tr>
                        <td class="py-2 fs-16 fw-bold">Volume (24h)</td>
                        <td class="ps-2 py-2 text-end">
                           <p class="fs-16">
                              {{threeSigFigs $botState.Volume}} DCR <span class="fs15">(~
                                 ${{threeSigFigs
                                 (floatMultiply $botState.Volume $botState.Price)}})</span>
                           </p>
                        </td>
                     </tr>
                     <tr>
                        <td class="py-2 fs-16 fw-bold">Vol/Mkt Cap (24h)</td>
                        <td class="ps-2 py-2 text-end">
                           <p><span class="fs-16">{{printf "%.2f" (x100 (divideFloat
                              $botState.Volume $.Info.CoinValueSupply))}}</span>%</p>
                        </td>
                     </tr>
                 </tbody>
               </table>
               <div class="d-flex justify-content-between">
                  <p class="mb-0 fs-13">Low</p>
                  <p class="mb-0 fs-13">High</p>
               </div>
               <div class="fs22 text-center pb-3">
                  <div class="range-price-bar" id="priceBar" data-low="{{$botState.LowPrice}}" data-high="{{$botState.HighPrice}}" data-price="{{$botState.Price}}">
                     <div class="d-flex justify-content-between">
                        <span class="fs-13 mt-2">{{printf "%.2f" $botState.LowPrice}}</span>
                        <span class="fs-13 mt-2">{{printf "%.2f" $botState.HighPrice}}</span>
                    </div>
                     <div class="range-marker" id="currentMarker"></div>
                     <div class="range-label fs-13" id="currentLabel">{{printf "%.2f" $botState.Price}}</div>
                 </div>
               </div>
            </div>
            {{- /* DCR/BTC MARKETS */ -}}
            <div class="ms-4 me-2 my-4 p-2 p-lg-4 bg-white frame-shadow">
               <table class="mx-auto mx-lg-3">
                  <tbody>
                     <tr class="fs14 lh1rem">
                       <td></td>
                       <td class="text-end fw-bold">DCR Vol.</td>
                       <td class="text-end fw-bold ps-3" data-market-target="fiatLabel"></td>
                       <td></td>
                     </tr>
                     {{range $botState.VolumeOrderedExchanges}}
                         <tr class="fs hover-row" data-market-target="xcRow" data-action="click->market#setExchange" data-token="{{.Token}}">
                             <td class="py-2 fs18 fw-bold"><div class="exchange-logo {{xcLogoName .Token}} me-2"></div> {{xcDisplayName .Token}}</td>
                             <td class="ps-2 fs16 py-2 text-end" data-type="volume">
                                 {{threeSigFigs .State.BaseVolume}}
                             </td>
                             <td class="ps-3 fs16 py-2 text-end" data-type="price">
                                 {{threeSigFigs .State.Price}}
                             </td>
                             <td class="fs16 px-2" data-type="arrow">
                               {{if eq .State.Change 0.0}}
                                   <span></span>
                               {{else if gt .State.Change 0.0}}
                                   <span class="dcricon-arrow-up text-green"></span>
                               {{else}}
                                   <span class="dcricon-arrow-down text-danger"></span>
                               {{end}}
                             </td>
                         </tr>
                     {{end}}
                     <tr class="fs sum-row hover-row" data-market-target="xcRow" data-action="click->market#setExchange" data-token="aggregated">
                         <td class="py-2 fs18 fw-bold"><div class="exchange-logo aggregated me-2"></div> Aggregate</td>
                         <td class="ps-2 fs16 py-2 text-end" data-type="volume">
                             {{threeSigFigs $botState.Volume}}
                         </td>
                         <td class="ps-1 fs16 py-2 text-end" data-type="price">
                             {{threeSigFigs $botState.Price}}
                         </td>
                         <td class="fs16 px-2" data-type="arrow">
                           {{if eq $botState.DCRUSD24hChange 0.0}}
                               <span></span>
                           {{else if gt $botState.DCRUSD24hChange 0.0}}
                               <span class="dcricon-arrow-up text-green"></span>
                           {{else}}
                               <span class="dcricon-arrow-down text-danger"></span>
                           {{end}}
                         </td>
                     </tr>
                 </tbody>
               </table>
            </div>
         </div>
         {{- /* RIGHT COLUMN */ -}}
         <div class="row flex-grow-0 flex-lg-grow-1 col-24 col-lg mx-0 px-3 my-4 frame-shadow">
            <div class="col me-md-2 flex-grow-1 d-flex flex-column bg-white position-relative">
               <div class="loader-v2 loading" data-market-target="chartLoader"></div>
               {{- /* CHART ONTROLS */ -}}
               <div
                  class="d-flex flex-shrink-1 align-items-top justify-content-around justify-content-lg-center flex-wrap pt-2 px-1 px-lg-3 mb-2">
                  {{- /* PAIR SELECTION */ -}}
                  <div class="btn-set bg-white d-inline-flex flex-nowrap mx-2 mx-lg-4">
                     <label>Pair</label>
                     <select class="chart-box d-inline-flex" data-market-target="pairSelect"
                        data-action="change->market#changePair">
                        <option value="usdt" selected>DCR/USDT</option>
                        <option value="btc">DCR/BTC</option>
                     </select>
                  </div>
                  {{- /* EXCHANGE SELECTION */ -}}
                  <div class="btn-set bg-white d-inline-flex flex-nowrap mx-2 mx-lg-4" data-market-target="exchanges"
                     data-action="click->market#changeExchange">
                     <label>Exchange</label>
                     <div class="btn-group" data-market-target="exchangeBtnArea">
                        {{range $index, $exchange := $botState.VolumeOrderedExchanges}}
                        {{if ne $exchange.Token "dcrdex"}}
                        <button name="{{$exchange.Token}}" {{if $exchange.State.HasCandlesticks}}
                           data-exchangeindex="{{$index}}" data-sticks="1" data-bins="{{$exchange.State.StickList}}"
                           {{end}} {{if $exchange.State.HasDepth}} data-depth="1" {{end}}
                           class="tab-button home-chart-toggle-btn white c-txt-main">{{xcDisplayName
                           $exchange.Token}}</button>
                        {{end}}
                        {{end}}
                        <button name="aggregated" data-market-target="aggOption" data-depth="1"
                           class="tab-button home-chart-toggle-btn white c-txt-main">Aggregated</button>
                     </div>
                  </div>
                  {{- /* CHART TYPE SELECTION */ -}}
                  <div class="btn-set bg-white d-inline-flex flex-nowrap mx-2 mx-lg-4">
                     <label>Chart</label>
                     <select class="chart-box d-inline-flex" data-market-target="chartSelect"
                        data-action="change->market#changeGraph">
                        <option value="depth" data-market-target="depthOnly" selected>
                           Depth
                           Chart</option>
                        <option value="orders" data-market-target="depthOnly">Order Book
                        </option>
                        <option value="candlestick" data-market-target="sticksOnly">
                           Candlesticks</option>
                        <option value="history" data-market-target="sticksOnly">Market
                           History</option>
                        <option value="volume" data-market-target="sticksOnly">Volume
                        </option>
                     </select>
                  </div>
                  {{- /* BIN SIZE SELECTIONS */ -}}
                  <div class="btn-set bg-white d-inline-flex flex-nowrap mx-2 mx-lg-4" data-market-target="bin"
                     data-action="click->market#changeBin">
                     <label class="text-nowrap">Bin Size</label>
                     <div class="btn-group">
                        <button name="5m" class="tab-button d-hide home-chart-toggle-btn white c-txt-main">5m</button>
                        <button name="30m" class="tab-button d-hide home-chart-toggle-btn white c-txt-main">30m</button>
                        <button name="1h" class="active tab-button home-chart-toggle-btn white c-txt-main">Hour</button>
                        <button name="4h" class="active tab-button home-chart-toggle-btn white c-txt-main">4
                           Hours</button>
                        <button name="1d" class="tab-button home-chart-toggle-btn white c-txt-main">Day</button>
                        <button name="1w" class="tab-button home-chart-toggle-btn white c-txt-main">Week</button>
                        <button name="1mo" class="tab-button home-chart-toggle-btn white c-txt-main">Month</button>
                     </div>
                  </div>
                  {{- /* DISPLAY CURRENCY */ -}}
