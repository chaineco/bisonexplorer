<script>
   const content = document.getElementById('content');
   const thumbs = document.querySelectorAll('.thumb');
   const pages = document.querySelectorAll('.page');

   content.addEventListener('scroll', () => {
      let index = Math.round(content.scrollTop / (window.innerHeight - 180));
      thumbs.forEach(thumb => thumb.classList.remove('active'));
      thumbs[index].classList.add('active');
   });
   async function updateMarketPriceRange() {
      try {
          let bar = document.getElementById("priceBar");
          let marker = document.getElementById("currentMarker");
          let label = document.getElementById("currentLabel");
          const currentPrice = Number(bar.getAttribute("data-price"));
          const lowPrice = Number(bar.getAttribute("data-low"));
          const highPrice = Number(bar.getAttribute("data-high"));

          let percent = ((currentPrice - lowPrice) / (highPrice - lowPrice)) * 100;
          marker.style.left = `${percent}%`;
          label.style.left = `${percent}%`;
          label.innerHTML = `${currentPrice.toFixed(2)}`;
      } catch (error) {
          console.error("Error when get price:", error);
      }
  }
  updateMarketPriceRange();
</script>
