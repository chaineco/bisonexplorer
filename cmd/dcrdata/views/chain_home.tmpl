{{define "chain_home" -}}
{{- $conv := .Conversions -}}
{{$ChainType := .ChainType}}
{{- $botState := .XcState -}}
<!DOCTYPE html>
<html lang="en">
{{ template "html-head" headData .CommonPageData (printf "%s Explorer by bisonexplorer.com" (chainName $ChainType))}}
{{ template "new_chainhome_navbar" . }}
<div class="d-flex justify-content-between ai-end new-home-header container py-2 px-2 px-lg-0" id="newHomeMenu">
   <div class="d-none d-md-flex mb-sm-0">
      <img id="pageBarIconTop" class="page-bar-icon" src="/images/blockchain-icon.svg" width="25" height="25">
      <span id="pageBarTitleTop" class="fs-18 fw-600 ms-2">Blockchain Summary</span>
   </div>
   <div class="button-group-navigation d-flex justify-content-md-end ai-end">
      {{if $conv}}
      <p class="me-2 mb-0 d-none d-md-flex ai-center">1 {{toUpperCase $ChainType}} = <span
            class="fs-18 ms-1 fw-bold top-price-color">$<span data-chainhome-target="exchangeRate">{{commaWithDecimal
               $conv.ExchangeRate.Value 2}}</span></span></p>
      {{end}}
      <li class="menu-list-item"><a class="home-menu-wrapper mx-1">
            <img src="/images/info-icon.svg" width="20" height="20" class="home-menu-icon" alt="blockchain info">
            <span class="d-none d-md-inline-block">Information</span></a>
         <div class="home-menu-dropdown">
            <div class="submenu-list__wrapper one-column">
               <ul class="submenu-list">
                  <li class="submenu-list__item has-submenu active">
                     <a href="/{{$ChainType}}/blocks" data-turbolinks="false" class="submenu-list__item-link w-100">
                        <div class="submenu-list__item-wrapper">
                           <div class="submenu-list__item-icon">
                              <img src="/images/blocks.svg" width="25" height="25" class="home-menu-icon"
                                 alt="blocks information">
                           </div>
                           <div>
                              <span class="submenu-list__item-title">Blocks</span>
                              <span class="submenu-list__item-subtile d-none d-md-block">Latest block information</span>
                           </div>
                        </div>
                     </a>
                  </li>
                  <li class="submenu-list__item has-submenu">
                     <a href="/{{$ChainType}}/mempool" data-turbolinks="false" class="submenu-list__item-link w-100">
                        <div class="submenu-list__item-wrapper">
                           <div class="submenu-list__item-icon">
                              <img src="/images/mempool.svg" width="25" height="25" class="home-menu-icon"
                                 alt="mempool information">
                           </div>
                           <div>
                              <span class="submenu-list__item-title">Mempool</span>
                              <span class="submenu-list__item-subtile d-none d-md-block">List of pending
                                 transactions</span>
                           </div>
                        </div>
                     </a>
                  </li>
               </ul>
            </div>
         </div>
      </li>
      <li class="menu-list-item"><a class="home-menu-wrapper mx-1">
            <img src="/images/chart-icon.svg" width="20" height="20" class="home-menu-icon" alt="decred chart">
            <span class="d-none d-md-inline-block">Graphics</span></a>
         <div class="home-menu-dropdown">
            <div class="submenu-list__wrapper one-column">
               <ul class="submenu-list">
                  <li class="submenu-list__item has-submenu active">
                     <a href="/{{$ChainType}}/charts" data-turbolinks="false" class="submenu-list__item-link w-100">
                        <div class="submenu-list__item-wrapper">
                           <div class="submenu-list__item-icon">
                              <img src="/images/chain-chart.svg" width="25" height="25" class="home-menu-icon"
                                 alt="chain charts">
                           </div>
                           <div>
                              <span class="submenu-list__item-title">Chain Charts</span>
                              <span class="submenu-list__item-subtile d-none d-md-block">Charts from chain data</span>
                           </div>
                        </div>
                     </a>
                  </li>
                  <li class="submenu-list__item has-submenu">
                     <a href="/{{$ChainType}}/market" data-turbolinks="false" class="submenu-list__item-link w-100">
                        <div class="submenu-list__item-wrapper">
                           <div class="submenu-list__item-icon">
                              <img src="/images/market.svg" width="25" height="25" class="home-menu-icon"
                                 alt="market charts">
                           </div>
                           <div>
                              <span class="submenu-list__item-title">Market</span>
                              <span class="submenu-list__item-subtile d-none d-md-block">Information from the
                                 market</span>
                           </div>
                        </div>
                     </a>
                  </li>
                  <li class="submenu-list__item has-submenu">
                     <a href="/{{$ChainType}}/visualblocks" data-turbolinks="false"
                        class="submenu-list__item-link w-100">
                        <div class="submenu-list__item-wrapper">
                           <div class="submenu-list__item-icon">
                              <img src="/images/bison-wallet.svg" width="25" height="25" class="home-menu-icon"
                                 alt="bison wallet">
                           </div>
                           <div>
                              <span class="submenu-list__item-title">Visual Blocks</span>
                              <span class="submenu-list__item-subtile d-none d-md-block">Blocks visual simulation</span>
                           </div>
                        </div>
                     </a>
                  </li>
               </ul>
            </div>
         </div>
      </li>
      <li class="menu-list-item"><a class="home-menu-wrapper mx-1">
            <img src="/images/tools-icon.svg" width="20" height="20" class="home-menu-icon" alt="multichain tools">
            <span class="d-none d-md-inline-block">Tools</span></a>
         <div class="home-menu-dropdown">
            <div class="submenu-list__wrapper one-column">
               <ul class="submenu-list">
                  <li class="submenu-list__item has-submenu active">
                     <a href="/" data-turbolinks="false" class="submenu-list__item-link w-100">
                        <div class="submenu-list__item-wrapper">
                           <div class="submenu-list__item-icon">
                              <img src="/images/other.svg" width="25" height="25" class="home-menu-icon"
                                 alt="other blockchains">
                           </div>
                           <div>
                              <span class="submenu-list__item-title">Other Blockchains</span>
                              <span class="submenu-list__item-subtile d-none d-md-block">All supported
                                 blockchains</span>
                           </div>
                        </div>
                     </a>
                  </li>
                  <li class="submenu-list__item has-submenu">
                     <a href="/{{$ChainType}}/supply" data-turbolinks="false" class="submenu-list__item-link w-100">
                        <div class="submenu-list__item-wrapper">
                           <div class="submenu-list__item-icon">
                              <img src="/images/supply.svg" width="25" height="25" class="home-menu-icon"
                                 alt="supply page">
                           </div>
                           <div>
                              <span class="submenu-list__item-title">Supply</span>
                              <span class="submenu-list__item-subtile d-none d-md-block">Distribution
                                 information</span>
                           </div>
                        </div>
                     </a>
                  </li>
                  <li class="submenu-list__item has-submenu active">
                     <a href="/{{$ChainType}}/parameters" data-turbolinks="false" class="submenu-list__item-link w-100">
                        <div class="submenu-list__item-wrapper">
                           <div class="submenu-list__item-icon">
                              <img src="/images/parameter.svg" width="25" height="25" class="home-menu-icon"
                                 alt="parameters">
                           </div>
                           <div>
                              <span class="submenu-list__item-title">Parameters</span>
                              <span class="submenu-list__item-subtile d-none d-md-block">Blockchain Parameters</span>
                           </div>
                        </div>
                     </a>
                  </li>
                  <li class="submenu-list__item has-submenu">
                     <a href="/whatsnew" data-turbolinks="false" class="submenu-list__item-link w-100">
                        <div class="submenu-list__item-wrapper">
                           <div class="submenu-list__item-icon">
                              <img src="/images/whats-new.svg" width="25" height="25" class="home-menu-icon"
                                 alt="whats new">
                           </div>
                           <div>
                              <span class="submenu-list__item-title">What's New</span>
                              <span class="submenu-list__item-subtile d-none d-md-block">Changes on Bison
                                 Explorer</span>
                           </div>
                        </div>
                     </a>
                  </li>
                  <li class="submenu-list__item has-submenu">
                     <a data-keynav-skip rel="noopener noreferrer" id="keynav-toggle" data-turbolinks="false"
                        class="submenu-list__item-link w-100">
                        <div class="submenu-list__item-wrapper">
                           <div class="submenu-list__item-icon">
                              <img src="/images/hotkey-icon.svg" width="25" height="25" class="home-menu-icon"
                                 alt="hotkeys">
                           </div>
                           <div>
                              <span class="submenu-list__item-title text">Enable Hot Keys</span>
                              <span class="submenu-list__item-subtile">
                                 <span class="keys-hint">(<span class="arrows">
                                       &#8592;<br>&#8594;</span>&nbsp;&nbsp;enter&nbsp;&nbsp;\&nbsp;&nbsp;=&nbsp;&nbsp;)</span>
                              </span>
                           </div>
                        </div>
                     </a>
                  </li>
               </ul>
            </div>
         </div>
      </li>
   </div>
   <div class="d-flex d-md-none ai-center">
      {{if $conv}}
      <p class="me-2 my-0 ai-center">1 {{toUpperCase $ChainType}} = <span
            class="fs-18 ms-1 fw-bold top-price-color">$<span
               data-chainhome-target="exchangeRateBottom">{{commaWithDecimal
               $conv.ExchangeRate.Value 2}}</span></span></p>
      {{end}}
   </div>
</div>
<div class="main-container new-homepage homepage-class homepage-content" data-chainhome-target="homeContent"
   id="newHomeContent" data-controller="time">
   <div class="hp-explore-wrap homepage-page ai-center">
      <div class="container px-0 snap-page-content">
         <section class="h-100 py-2">
            <div class="d-flex ai-center h-100">
               <div class="row">
                  <div class="col-24 col-md-12 col-lg-8 p-2">
                     <div class="new-home-card-container h-100">
                        <a href='/{{$ChainType}}/blocks' data-turbolinks="false" class="link-to-subpage">
                           <div class="py-2 px-3 new-home-card-header d-flex ai-center">
                              <span class="homeicon-block explore-card__header__logo" alt="bitcoin logo"
                                 loading="lazy"></span>
                              <p class="ms-2 mb-0 fw-600 fs-17">Blocks</p>
                           </div>
                        </a>
                        {{with .BestBlock -}}
                        <div class="row new-home-card-content px-2 px-md-3 py-2">
                           <div class="col-12 card-content-box">
                              <div class="new-network-stats-block flex-column-card">
                                 <span class="new-network-stats-title">Latest Block</span>
                                 <div class="flex-card-content">
                                    <div>
                                       <span class="new-network-stats-content d-inline-block">
                                          <a class="fs-22" data-turbolinks="false"
                                             href="/{{$ChainType}}/block/{{.Hash}}"
                                             data-chainhome-target="blockHeight">{{.Height}}</a>
                                       </span>
                                       <span class="stats-subinfo fs-15i" data-chainhome-target="blockTime"
                                          data-type="age" data-time-target="age" data-age="{{.BlockTimeUnix}}"></span>
                                    </div>
                                 </div>
                              </div>
                           </div>
                           <div class="col-12 card-content-box">
                              <div class="new-network-stats-block flex-column-card">
                                 <span class="new-network-stats-title">Value</span>
                                 <div class="flex-card-content">
                                    <div>
                                       <span class="new-network-stats-content d-inline-block">
                                          <span data-chainhome-target="blockTotal">
                                             {{threeSigFigs .Total}}</span>
                                          <span class="new-info-line-2-sub">{{toUpperCase $ChainType}}</span>
                                       </span>
                                    </div>
                                 </div>
                              </div>
                           </div>
                           <div class="col-12 card-content-box">
                              <div class="new-network-stats-block flex-column-card">
                                 <span class="new-network-stats-title">Size</span>
                                 <div class="flex-card-content">
                                    <div>
                                       <span class="new-network-stats-content d-inline-block pt-1"
                                          data-chainhome-target="blockSize">{{.FormattedBytes}}</span>
                                    </div>
                                 </div>
                              </div>
                           </div>
                           <div class="col-12 card-content-box">
                              <div class="new-network-stats-block flex-column-card">
                                 <span class="new-network-stats-title">Transactions</span>
                                 <div class="flex-card-content">
                                    <div>
                                       <span class="new-network-stats-content d-inline-block pt-1"
                                          data-chainhome-target="blockTxCount">{{int64Comma (toint64
                                          .Transactions)}}</span>
                                    </div>
                                 </div>
                              </div>
                           </div>
                        </div>
                        {{end}}
                     </div>
                  </div>
                  <div class="col-24 col-md-12 col-lg-16 p-2">
                     <div class="new-home-card-container h-100">
                        <div class="py-2 px-3 new-home-card-header d-flex ai-center">
                           <span class="dcricon-tree explore-card__header__logo" alt="distribution of decred"
                              loading="lazy"></span>
                           <p class="ms-2 mb-0 fw-600 fs-17">Blockchain Summary</p>
                        </div>
                        {{with .Info -}}
                        <div class="row new-home-card-content px-2 px-md-3 py-2">
                           <div class="col-12 col-sm-8 card-content-box">
                              <div class="new-network-stats-block flex-column-card">
                                 <span class="new-network-stats-title">
                                    <a class="link-hover-underline" data-turbolinks="false"
                                       href="/{{$ChainType}}/charts?chart=coin-supply">
                                       Coin Supply (of 21 mil)
                                    </a>
                                 </span>
                                 <div class="flex-card-content">
                                    <div>
                                       <span class="new-network-stats-content d-inline-block">
                                          <span data-chainhome-target="coinSupply">
                                             {{template "decimalParts" (float64AsDecimalParts .CoinValueSupply 2 true)}}
                                          </span>
                                          <span class="new-info-line-2-sub">{{toUpperCase $ChainType}}</span>
                                          <span class="stats-subinfo">
                                             {{if $conv -}}
                                             <span data-chainhome-target="convertedSupply">{{threeSigFigs
                                                $conv.CoinSupply.Value}} {{$conv.CoinSupply.Index}}</span>
                                             {{- end}}
                                          </span>
                                       </span>
                                    </div>
                                 </div>
                              </div>
                           </div>
                           <div class="col-12 col-sm-8 card-content-box">
                              <div class="new-network-stats-block flex-column-card">
                                 <span class="new-network-stats-title">
                                    <a class="link-hover-underline" data-turbolinks="false"
                                       href="/{{$ChainType}}/charts?chart=blockchain-size">
                                       Blockchain Size
                                    </a>
                                 </span>
                                 <div class="flex-card-content">
                                    <div>
                                       <span class="new-network-stats-content d-inline-block"
                                          data-chainhome-target="blockchainSize">
                                          {{.FormattedSize}}
                                       </span>
                                    </div>
                                 </div>
                              </div>
                           </div>
                           <div class="col-12 col-sm-8 card-content-box">
                              <div class="new-network-stats-block flex-column-card">
                                 <span class="new-network-stats-title">
                                    <a class="link-hover-underline" data-turbolinks="false"
                                       href="/{{$ChainType}}/charts?chart=tx-count">
                                       Total Transactions
                                    </a>
                                 </span>
                                 <div class="flex-card-content">
                                    <div>
                                       <span class="new-network-stats-content d-inline-block"
                                          data-chainhome-target="totalTransactions">
                                          {{int64Comma .TotalTransactions}}
                                       </span>
                                       <span class="stats-subinfo">
                                          {{commaWithDecimal .UseRingctRate 2}} % use ringct
                                       </span>
                                    </div>
                                 </div>
                              </div>
                           </div>
                           {{if ne $ChainType "xmr"}}
                           <div class="col-12 col-sm-8 card-content-box">
                              <div class="new-network-stats-block flex-column-card">
                                 <span class="new-network-stats-title">
                                    Total Addresses
                                 </span>
                                 <div class="flex-card-content">
                                    <div>
                                       <span class="new-network-stats-content d-inline-block"
                                          data-chainhome-target="totalAddresses">
                                          {{int64Comma .TotalAddresses}}
                                       </span>
                                    </div>
                                 </div>
                              </div>
                           </div>
                           {{else}}
                           <div class="col-12 col-sm-8 card-content-box">
                              <div class="new-network-stats-block flex-column-card">
                                 <span class="new-network-stats-title">
                                    Total Inputs (Key Images)
                                 </span>
                                 <div class="flex-card-content">
                                    <div>
                                       <span class="new-network-stats-content d-inline-block"
                                          data-chainhome-target="totalInputs">
                                          {{int64Comma .TotalInputs}}
                                       </span>
                                    </div>
                                 </div>
                              </div>
                           </div>
                           {{end}}
                           <div class="col-12 col-sm-8 card-content-box">
                              <div class="new-network-stats-block flex-column-card">
                                 <span class="new-network-stats-title">
                                    Total Outputs
                                 </span>
                                 <div class="flex-card-content">
                                    <div>
                                       <span class="new-network-stats-content d-inline-block"
                                          data-chainhome-target="totalOutputs">
                                          {{int64Comma .TotalOutputs}}
                                       </span>
                                    </div>
                                 </div>
                              </div>
                           </div>
                            {{if ne $ChainType "xmr"}}
                           <div class="col-12 col-sm-8 card-content-box">
                              <div class="new-network-stats-block flex-column-card">
                                 <span class="new-network-stats-title">
                                    Online Nodes
                                 </span>
                                 <div class="flex-card-content">
                                    <div>
                                       <span class="new-network-stats-content d-inline-block"
                                          data-chainhome-target="onlineNodes">
                                          {{int64Comma .Nodes}}
                                       </span>
                                    </div>
                                 </div>
                              </div>
                           </div>
                           {{else}}
                           <div class="col-12 col-sm-8 card-content-box">
                              <div class="new-network-stats-block flex-column-card">
                                 <span class="new-network-stats-title">
                                    Total Ring Members
                                 </span>
                                 <div class="flex-card-content">
                                    <div>
                                       <span class="new-network-stats-content d-inline-block"
                                          data-chainhome-target="totalRingMembers">
                                          {{int64Comma .TotalRingSize}}
                                       </span>
                                    </div>
                                 </div>
                              </div>
                           </div>
                           {{end}}
                        </div>
                        {{end}}
                     </div>
                  </div>
                  <div class="col-24 col-lg-12 p-2">
                     <div class="new-home-card-container h-100">
                        <a href='/{{$ChainType}}/mempool' data-turbolinks="false" class="link-to-subpage">
                           <div class="py-2 px-3 new-home-card-header d-flex ai-center">
                              <span class="homeicon-mempool explore-card__header__logo" alt="Mempool route"
                                 loading="lazy"></span>
                              <p class="ms-2 mb-0 fw-600 fs-17">Mempool</p>
                           </div>
                        </a>
                        {{with .MempoolInfo -}}
                        <div class="justify-content-between new-home-card-content px-2 px-md-3 py-2 flex-column-card">
                           <div class="row flex-1">
                              <div class="col-12 card-content-box">
                                 <div class="new-network-stats-block flex-column-card">
                                    <span class="new-network-stats-title">
                                       <a class="link-hover-underline" data-turbolinks="false"
                                          href="/{{$ChainType}}/charts?chart=mempool-txs">
                                          Transactions
                                       </a>
                                    </span>
                                    <div class="flex-card-content">
                                       <div>
                                          <span class="new-network-stats-content d-inline-block pt-1">
                                             <span data-chainhome-target="txCount">
                                                {{int64Comma .TotalTransactions}}
                                             </span>
                                          </span>
                                       </div>
                                    </div>
                                 </div>
                              </div>
                              {{if eq $ChainType "xmr"}}
                              <div class="col-12 card-content-box">
                                 <div class="new-network-stats-block flex-column-card">
                                    <span class="new-network-stats-title">
                                       Inputs (Key Images)
                                    </span>
                                    <div class="flex-card-content">
                                       <div>
                                          <span class="new-network-stats-content d-inline-block pt-1">
                                             <span data-chainhome-target="memInputCount">
                                                {{int64Comma .InputsCount}}
                                             </span>
                                          </span>
                                       </div>
                                    </div>
                                 </div>
                              </div>
                              {{end}}
                              <div class="col-12 card-content-box">
                                 <div class="new-network-stats-block flex-column-card">
                                    <span class="new-network-stats-title">
                                       Processing Outputs
                                    </span>
                                    <div class="flex-card-content">
                                       <div>
                                          <span class="new-network-stats-content d-inline-block pt-1">
                                             <span data-chainhome-target="txOutCount">
                                                {{int64Comma .OutputsCount}}
                                             </span>
                                          </span>
                                       </div>
                                    </div>
                                 </div>
                              </div>
                              {{if ne $ChainType "xmr"}}
                              <div class="col-12 card-content-box">
                                 <div class="new-network-stats-block flex-column-card">
                                    <span class="new-network-stats-title">Total Sent</span>
                                    <div class="flex-card-content">
                                       <div>
                                          <span class="new-network-stats-content d-inline-block">
                                             <span data-chainhome-target="totalSent">
                                                {{template "decimalParts" (float64AsDecimalParts .TotalOut 3 true)}}
                                             </span>
                                             <span class="new-info-line-2-sub">{{toUpperCase $ChainType}}</span>
                                             <span class="stats-subinfo">
                                                {{if $conv -}}
                                                <span data-chainhome-target="totalSentExchange">
                                                   {{threeSigFigs $conv.MempoolSent.Value}}
                                                   {{$conv.MempoolSent.Index}}
                                                </span>
                                                {{- end}}
                                             </span>
                                          </span>
                                       </div>
                                    </div>
                                 </div>
                              </div>
                              {{end}}
                              <div class="col-12 card-content-box">
                                 <div class="new-network-stats-block flex-column-card">
                                    <span class="new-network-stats-title">Total Fees</span>
                                    <div class="flex-card-content">
                                       <div>
                                          <span class="new-network-stats-content d-inline-block">
                                             <span data-chainhome-target="totalFee">
                                                {{template "decimalParts" (float64AsDecimalParts .TotalFee 8 false)}}
                                             </span>
                                             <span class="new-info-line-2-sub">{{toUpperCase $ChainType}}</span>
                                             <span class="stats-subinfo">
                                                {{if $conv -}}
                                                <span data-chainhome-target="totalFeesExchange">
                                                   {{threeSigFigs $conv.MempoolFees.Value}}
                                                   {{$conv.MempoolFees.Index}}
                                                </span>
                                                {{- end}}
                                             </span>
                                          </span>
                                       </div>
                                    </div>
                                 </div>
                              </div>
                              <div class="col-12 card-content-box">
                                 <div class="new-network-stats-block flex-column-card">
                                    <span class="new-network-stats-title">Fee Span</span>
                                    <div class="flex-card-content">
                                       <div>
                                          <span class="new-network-stats-content d-inline-block pt-1">
                                             <span data-chainhome-target="minFeeRate">{{template "decimalParts"
                                                (float64AsDecimalParts .MinFeeRatevB 0 false)}}</span> -
                                             <span data-chainhome-target="maxFeeRate">{{template "decimalParts"
                                                (float64AsDecimalParts .MaxFeeRatevB 0 false)}}</span>
