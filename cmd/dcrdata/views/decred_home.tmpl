<div class="main-container new-homepage homepage-class homepage-content" data-homepage-target="homeContent" id="newHomeContent"
   data-controller="charts time" data-charts-tps="{{.TargetPoolSize}}"
   data-charts-svh="{{.ChainParams.StakeValidationHeight}}" data-charts-pos="{{.ChainParams.StakeRewardProportion}}"
   data-charts-premine="{{.Premine}}" data-charts-bs="{{.ChainParams.BaseSubsidy}}"
   data-charts-sri="{{.ChainParams.SubsidyReductionInterval}}" data-charts-mul-subsidy="{{.ChainParams.MulSubsidy}}"
   data-charts-div-subsidy="{{.ChainParams.DivSubsidy}}" data-charts-window-size="{{.ChainParams.StakeDiffWindowSize}}"
   data-charts-block-time="{{.ChainParams.TargetTimePerBlock.Seconds}}">
   <div class="hp-explore-wrap homepage-page">
      <div class="container px-0 snap-page-content">
         <section class="h-100 py-2">
            <div class="row">
               <div class="col-24 col-md-12 col-lg-8 p-2">
                  <div class="new-home-card-container">
                     <a href='/blocks' data-turbolinks="false" class="link-to-subpage">
                        <div class="py-2 px-3 new-home-card-header d-flex ai-center">
                           <span class="homeicon-block explore-card__header__logo" alt="bitcoin logo"
                              loading="lazy"></span>
                           <p class="ms-2 mb-0 fw-600 fs-17">Blocks</p>
                        </div>
                     </a>
                     {{with .BestBlock -}}
                     <div class="row justify-content-between px-3 py-2">
                        <div class="col-12">
                           <div class="new-network-stats-block pb-2">
                              <span class="new-network-stats-title">Latest Blocks</span>
                              <span class="new-network-stats-content d-inline-block">
                                 <a class="fs-22" data-turbolinks="false" href="/block/{{.Hash}}"
                                    data-homepage-target="blockHeight">{{.Height}}</a>
                              </span>
                              <span class="stats-subinfo fs-15i" data-homepage-target="lastBlockTime" data-type="age" data-time-target="age"
                                 data-age="{{.BlockTime.UNIX}}"></span>
                           </div>
                        </div>
                        <div class="col-12">
                           <div class="new-network-stats-block pb-2">
                              <span class="new-network-stats-title">Value</span>
                              <span class="new-network-stats-content d-inline-block pt-1">
                                 <span data-homepage-target="blockTotal">
                                    {{threeSigFigs .Total}}</span>
                                 <span class="new-info-line-2-sub">DCR</span>
                              </span>
                              <!--<span class="stats-subinfo">Phash/s</span> -->
                           </div>
                        </div>
                        <div class="col-12">
                           <div class="new-network-stats-block pb-2">
                              <span class="new-network-stats-title">Votes
                                 {{if eq $.Consensus -1 -}}
                                 <span data-homepage-target="consensusMsg" class="fs-13 text-danger">rejected</span>
                                 {{else if eq $.Consensus 0}}
                                 <span data-homepage-target="consensusMsg" class="fs-13"></span>
                                 {{else}}
                                 <span data-homepage-target="consensusMsg" class="fs-13 text-green">approved</span>
                                 {{- end}}
                              </span>
                              <span class="d-inl new-network-stats-content d-inline-block pt-1"
                                 data-homepage-target="blockVotes" data-hash="{{.Hash}}">
                                 {{range $index, $vote := $.BlockTally -}}
                                 <span class="d-inline-block position-relative"
                                    data-tooltip="{{if eq $vote -1}}the stakeholder has voted to reject this block{{else if eq $vote 1}}the stakeholder has voted to accept this block{{else}}this vote has not been received yet{{end}}">
                                    <span
                                       class="fs-22 dcricon-{{if eq $vote -1}}reject{{else if eq $vote 1}}affirm{{else}}missing{{end}}"></span>
                                 </span>
                                 {{- end}}
                              </span>
                           </div>
                        </div>
                        <div class="col-12">
                           <div class="new-network-stats-block pb-2">
                              <span class="new-network-stats-title">Size</span>
                              <span class="new-network-stats-content d-inline-block pt-1"
                                 data-homepage-target="blockSize">{{.FormattedBytes}}</span>
                           </div>
                        </div>
                     </div>
                     {{end}}
                  </div>
               </div>
               <div class="col-24 col-md-12 col-lg-16 p-2">
                  <div class="new-home-card-container">
                     <a href='/supply' data-turbolinks="false" class="link-to-subpage">
                        <div class="py-2 px-3 new-home-card-header d-flex ai-center">
                           <span class="dcricon-tree explore-card__header__logo" alt="distribution of decred"
                              loading="lazy"></span>
                           <p class="ms-2 mb-0 fw-600 fs-17">Distribution</p>
                        </div>
                     </a>
                     {{with .Info -}}
                     <div class="row justify-content-between px-3 py-2">
                        <div class="col-24 col-sm-12">
                           <div class="new-network-stats-block">
                              <span class="new-network-stats-title">
                                 <a class="link-hover-underline" data-turbolinks="false" href="/treasury?txntype=all">
                                    Treasury</a>
                              </span>
                              <span class="new-network-stats-content d-inline-block">
                                 <span data-homepage-target="devFund">
                                    {{template "decimalParts" (float64AsDecimalParts (toFloat64Amount (add
                                    .DevFund .TreasuryBalance.Balance)) 2 true 2)}}
                                 </span>
                                 <span class="new-info-line-2-sub">DCR</span>
                                 <span class="position-relative"
                                    data-tooltip='include {{printf "%.2f" (toFloat64Amount .DevFund)}} DCR in legacy treasury'>
                                    <span class="dcricon-info fs13 cursor-pointer">
                                    </span>
                                 </span>
                              </span>
                              {{if $conv -}}
                              <span class="stats-subinfo">
                                 <span data-homepage-target="convertedDev">
                                    {{threeSigFigs $conv.TreasuryBalance.Value}}
                                    {{$conv.TreasuryBalance.Index}}
                                 </span>
                              </span>
                              {{- end}}
                           </div>
                        </div>
                        <div class="col-24 col-sm-12">
                           <div class="new-network-stats-block">
                              <span class="new-network-stats-title">
                                 <a class="link-hover-underline" data-turbolinks="false"
                                    href="/charts?chart=coin-supply">
                                    Total Coin Supply<span class="nowrap fw-600">&nbsp(of 21 mil)</span>
                                 </a>
                              </span>
                              <span class="new-network-stats-content d-inline-block">
                                 <span data-homepage-target="coinSupply">
                                    {{template "decimalParts" (float64AsDecimalParts (toFloat64Amount
                                    .CoinSupply) 0 true)}}
                                 </span>
                                 <span class="new-info-line-2-sub">DCR</span>
                              </span>
                              <span class="stats-subinfo">
                                 {{if $conv -}}
                                 <span data-homepage-target="convertedSupply">{{threeSigFigs
                                    $conv.CoinSupply.Value}} {{$conv.CoinSupply.Index}}</span>,
                                 {{- end}}
                                 <span data-homepage-target="mixedPct">{{printf "%.0f"
                                    .MixedPercent}}</span>%
                                 mixed
